<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.wd.dao.CFuturesDescriptionDao">
	<select id="queryObject" resultType="io.renren.modules.wd.entity.CFuturesDescriptionEntity" >
		select * from wind_filesync.CFUTURESDESCRIPTION where object_id = #{value}
	</select> 

	<select id="queryList" resultType="io.renren.modules.wd.entity.CFuturesDescriptionEntity" >
		select * from wind_filesync.CFUTURESDESCRIPTION 
		<where>
			imp_time like '%${impTime}%'
		</where>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(1) from wind_filesync.CFUTURESDESCRIPTION 
		<where>
			imp_time like '%${impTime}%' 
		</where>
	</select>
		<insert id="save" parameterType="io.renren.modules.wd.entity.CFuturesDescriptionEntity">
		insert into wind_filesync.CFUTURESDESCRIPTION
		(
			`object_id`, 
			`s_info_windcode`, 
			`s_info_code`, 
			`s_info_name`,
			`s_info_ename`,
			`fs_info_sccode`, 
			`fs_info_type`,
			`fs_info_cctype`,
			`s_info_exchmarket`,
			`s_info_listdate`,
			`s_info_delistdate`,
			`fs_info_dlmonth`,
			`fs_info_lprice`,
			`fs_info_ltdldate`,
			`opdate`,
			`opmode`,
			`imp_date`,
			`imp_time`
		)
		values
		(
			#{objectId}, 
			#{sInfoWindcode}, 
			#{sInfoCode}, 
			#{sInfoName}, 
			#{sInfoEname},
			#{fsInfoSccode},
			#{fsInfoType},
			#{fsInfoCctype},
			#{sInfoExchmarket},
			#{sInfoListdate},
			#{sInfoDelistdate},
			#{fsInfoDlmonth},
			#{fsInfoLprice},
			#{fsInfoLtdldate},
			#{opdate},
			#{opmode},
			#{impDate},
			#{impTime}	
		)
	</insert>
</mapper>